# Database Configuration
DB_URL=jdbc:postgresql://localhost:5432/blog
DB_USERNAME=blog
DB_PASSWORD=blog

# JWT Configuration
# IMPORTANT: Generate a new secret key for production using a secure random generator
# Example: openssl rand -hex 32
JWT_SECRET_KEY=your-secret-key-here-minimum-32-chars
JWT_EXPIRATION=3600000

# Logging Configuration (use DEBUG for development, INFO for production)
LOG_LEVEL_WEB=DEBUG
LOG_LEVEL_SECURITY=DEBUG
LOG_LEVEL_APP=DEBUG
SHOW_SQL=true
FORMAT_SQL=true
